<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="AppTheme" parent="android:Theme.Light" />
    <style name="HomeButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:minWidth">100dp</item>
        <item name="android:minHeight">70dp</item>
    </style>
    <style name="NormalButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:minWidth">100dp</item>
        <item name="android:minHeight">70dp</item>
    </style>
    <style name="SmallButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:minWidth">70dp</item>
        <item name="android:minHeight">50dp</item>
    </style>
    <style name="LargeButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">16dp</item>
        <item name="android:padding">30dp</item>
        <item name="android:minWidth">180dp</item>
        <item name="android:minHeight">120dp</item>
    </style>
    <style name="ExtraLargeButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">20dp</item>
        <item name="android:padding">40dp</item>
        <item name="android:minWidth">200dp</item>
        <item name="android:minHeight">140dp</item>
    </style>
    <style name="ReportHeader" parent="@android:style/TextAppearance.Large">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/report_header</item>
        <item name="android:background">@color/report_header_bg</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:typeface">sans</item>
    </style>
    <style name="ReportSubHeader" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">2dp</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingTop">2dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textStyle">italic</item>
        </style>
    <style name="ReportFieldLayout" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>
    <style name="ReportFieldTitle" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">3</item>
        <item name="android:textColor">@color/report_field_title</item>
        <item name="android:background">@color/report_field_title_bg</item>
        <item name="android:typeface">sans</item>
        <item name="android:textStyle">bold</item>
        </style>
    <style name="ReportFieldValue" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">5</item>
        <item name="android:textColor">@color/report_field_value</item>
        <item name="android:background">@color/report_field_value_bg</item>
        <item name="android:typeface">sans</item>
        <item name="android:paddingLeft">5dp</item>
        </style>
    <style name="SettingTitle" parent="@android:style/TextAppearance.Large">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">0dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="SettingDescription" parent="@android:style/TextAppearance.Small">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:typeface">sans</item>
    </style>
    <style name="SettingRadioButton" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:typeface">sans</item>
    </style>
    <style name="SettingRadioGroup" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:typeface">sans</item>
        <item name="android:padding">5dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>
    <style name="SettingNumberPicker" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_margin">5dp</item>
        <item name="android:typeface">sans</item>
    </style>
    <style name="SettingCheckBox" parent="@android:style/TextAppearance.Medium">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:typeface">sans</item>
    </style>
    <style name="AboutHeader" parent="@android:style/TextAppearance.Large">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="android:layout_marginBottom">5dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textStyle">bold</item>
        </style>
    <style name="AboutBodyText" parent="@android:style/TextAppearance.Small">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:paddingLeft">0dp</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:paddingRight">0dp</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginBottom">2dp</item>
        <item name="android:layout_marginLeft">5dp</item>
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:typeface">sans</item>
        <item name="android:textStyle">italic</item>
        </style>
    <style name="AboutButton" parent="@android:style/TextAppearance.Large">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:textColor">#FFF</item>
        <item name="android:background">@drawable/about_blue_button</item>
        <item name="android:textStyle">bold</item>
        <item name="android:paddingBottom">20dp</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:layout_marginBottom">10dp</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
        </style>
</resources>